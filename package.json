{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:ntl": "cross-env NODE_ENV=development netlify dev",
    "dev:css": "npm run generate:css -- --watch",
    "generate:css": "npx tailwindcss -i ./app/styles/wind.css -o ./app/styles/tailwind.css",
    "start": "cross-env NODE_ENV=production netlify dev"
  },
  "dependencies": {
    "@netlify/functions": "^1.0.0",
    "@remix-run/netlify": "^1.6.1",
    "@remix-run/node": "^1.6.1",
    "@remix-run/react": "^1.6.1",
    "cross-env": "^7.0.3",
    "gsap": "^3.10.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "three": "^0.141.0",
    "tiny-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.1",
    "@remix-run/eslint-config": "^1.6.1",
    "eslint": "^8.15.0",
    "tailwindcss": "^3.1.4"
  },
  "engines": {
    "node": ">=14"
  }
}
